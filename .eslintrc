{
    parser: "babel-eslint",
    "env": {
        "es6": true,
        "commonjs": true,
        "node": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "max-len": [
            1,
            120,
            2,
            {
                "ignoreComments": true
            }
        ],
        "prop-types": [
            0
        ],
        "linebreak-style": 0,
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "comma-dangle": [
            1,
            "never"
        ],
        "react/jsx-uses-vars": 1,
        "react/jsx-uses-react": 1,
        "object-curly-spacing": 1,
        "prefer-destructuring": [
            "warn",
            {
                "object": true,
                "array": false
            }
        ],
        "object-curly-newline": 0
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb-base"
    ]
}
